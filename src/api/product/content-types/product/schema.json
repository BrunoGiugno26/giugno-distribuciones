{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "productName": {
      "type": "string"
    },
    "slug": {
      "type": "uid",
      "targetField": "productName"
    },
    "description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "active": {
      "type": "boolean",
      "default": true
    },
    "price": {
      "type": "integer"
    },
    "origin": {
      "type": "enumeration",
      "enum": [
        "argentina",
        "uruguay",
        "brasil"
      ]
    },
    "tipoProducto": {
      "type": "enumeration",
      "enum": [
        "shampoo",
        "acondicionador",
        "bano_de_crema",
        "tonico_capilar",
        "tinturas",
        "accesorios",
        "decolorantes",
        "ampollas",
        "revelador_antioxidantes"
      ]
    },
    "tipoCabello": {
      "type": "enumeration",
      "enum": [
        "cabello_seco\t",
        "cabello_graso\t",
        "danado_quebradizo\t",
        "tenido_decolorado\t",
        "metodo_curly",
        "todo_tipo"
      ]
    },
    "isFeatured": {
      "type": "boolean"
    },
    "category": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::category.category"
    }
  }
}
